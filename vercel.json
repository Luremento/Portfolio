{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4",
      "includeFiles": "vendor/**,resources/views/**,bootstrap/cache/**"
    }
  },
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}